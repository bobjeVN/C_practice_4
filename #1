#include <iostream>
#include <ctime>
#include <string>
#include <windows.h>

using namespace std;

int main(SetConsoleOutputCP(CP_UTF8)

void task1(int *num, int size) {
    int min, temp;
      for (int i = 0; i < size - 1; i++)
      {
        min = i; 
        for (int j = i + 1; j < size; j++)  
        {
          if (num[j] < num[min]) 
            min = j;       
        }
        temp = num[i];      
        num[i] = num[min];
        num[min] = temp;
      }
}
